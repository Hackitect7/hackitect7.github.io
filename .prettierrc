{
  "plugins": ["prettier-plugin-go-template"],
  "overrides": [
    {
      "files": ["**/layouts/**/*.html", "**/themes/**/layouts/**/*.html"],
      "options": {
        "parser": "go-template"
      }
    }
  ],
  "printWidth": 100,
  "singleQuote": false,
  "semi": true,
  "trailingComma": "none"
}
